{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/env.d.ts",
    "src/renderer//**/*",
    "src/renderer//**/*.tsx",
    "src/preload/*.d.ts",
    "src/renderer/App.tsx",
    "src/renderer/App.tsx"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/renderer/*"
      ]
    }
  }
}